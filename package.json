{
  "name": "mobb-ai-tracer",
  "displayName": "Mobb AI Tracer",
  "version": "0.2.18",
  "repository": "https://github.com/mobb-dev/tracy",
  "publisher": "Mobb",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.99.0",
    "node": "22.x"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Mobb AI Tracer",
      "properties": {
        "mobbAiTracer.apiUrl": {
          "type": "string",
          "default": "https://api.mobb.ai/v1/graphql",
          "description": "API endpoint URL for Mobb services. Requires window reload to take effect."
        },
        "mobbAiTracer.webAppUrl": {
          "type": "string",
          "default": "https://app.mobb.ai",
          "description": "Web application URL for Mobb. Requires window reload to take effect."
        }
      }
    }
  },
  "scripts": {
    "pull-env": "dotenv-vault pull",
    "compile": "npm run pull-env && npm run build",
    "build": "tsc -p .",
    "watch": "tsc -p . -w",
    "lint": "eslint --cache --max-warnings 0 --ignore-path .eslintignore --ext .js,.ts,.tsx,.jsx .",
    "lint:fix": "eslint --fix --cache --max-warnings 0 --ignore-path .eslintignore --ext .js,.ts,.tsx,.jsx .",
    "lint:fix:files": "eslint --fix --cache --max-warnings 0 --ignore-path .eslintignore --ext .js,.ts,.tsx,.jsx",
    "test": "vitest run __tests__/processor.test.ts __tests__/smoke.test.ts && vitest run __tests__/extension.test.ts -t \"without\" && vitest run __tests__/extension.test.ts -t \"with thinking\" && vitest run __tests__/human.segmenter.test.ts __tests__/human.uploader.test.ts __tests__/human.recorder.int.test.ts __tests__/human.format.int.test.ts __tests__/human.eventClassifier.test.ts",
    "test:ci": "vitest run --bail=1",
    "test:watch": "vitest watch",
    "package": "vsce package --allow-package-env-file",
    "publish:microsoft": "vsce publish --allow-package-env-file --skip-duplicate",
    "publish:openvsx": "ovsx publish --packagePath $(ls *.vsix | sort -V | tail -n1) --skip-duplicate",
    "local-install": "pnpm i --no-link-workspace-packages",
    "package:dev": "./package-dev.sh --env local",
    "package:dev:staging": "./package-dev.sh --env staging",
    "package:dev:prod": "./package-dev.sh --env prod",
    "package:dev:uninstall": "code --uninstall-extension Mobb.mobb-ai-tracer-dev 2>/dev/null; cursor --uninstall-extension Mobb.mobb-ai-tracer-dev 2>/dev/null; echo 'Dev extension uninstalled'"
  },
  "Readme comment dependencies": "IMPORTANT: We must use exact versions in the dependencies because vcse bundler only works with npm correctly. pnpm is not supported.",
  "dependencies": {
    "@gitbeaker/requester-utils": "43.8.0",
    "@gitbeaker/rest": "43.8.0",
    "@modelcontextprotocol/sdk": "1.25.1",
    "@octokit/core": "5.2.0",
    "@octokit/request-error": "5.1.1",
    "@openredaction/openredaction": "1.0.4",
    "adm-zip": "0.5.16",
    "azure-devops-node-api": "15.1.1",
    "bitbucket": "2.12.0",
    "chalk": "5.6.2",
    "chalk-animation": "2.0.3",
    "configstore": "7.1.0",
    "cross-fetch": "4.1.0",
    "debug": "4.4.3",
    "diff": "5.2.0",
    "dotenv": "16.6.1",
    "escape-html": "1.0.3",
    "extract-zip": "2.0.1",
    "gitleaks-secret-scanner": "1.2.2",
    "globby": "14.1.0",
    "graphql-request": "6.1.0",
    "graphql-ws": "5.16.2",
    "hash-wasm": "4.12.0",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.6",
    "ignore": "7.0.5",
    "inquirer": "9.3.8",
    "isomorphic-ws": "5.0.0",
    "istextorbinary": "9.5.0",
    "libsodium-wrappers": "0.7.15",
    "multimatch": "7.0.0",
    "nanospinner": "1.1.0",
    "node-fetch": "3.3.2",
    "node-stream-zip": "1.15.0",
    "octokit": "3.2.1",
    "open": "8.4.2",
    "parse-diff": "0.11.1",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "sax": "1.4.3",
    "semver": "7.7.3",
    "simple-git": "3.30.0",
    "sql.js": "1.13.0",
    "tmp": "0.2.5",
    "tmp-promise": "3.0.3",
    "undici": "6.21.3",
    "uuid": "11.1.0",
    "ws": "8.18.3",
    "xml2js": "0.6.2",
    "yaml": "^2.8.1",
    "yargs": "17.7.2",
    "zod": "3.25.76"
  },
  "Readme comment devDependencies": "IMPORTANT: We must use exact versions in the dependencies because vcse bundler only works with npm correctly. pnpm is not supported.",
  "devDependencies": {
    "@octokit/types": "13.10.0",
    "@types/adm-zip": "0.5.7",
    "@types/chalk-animation": "1.6.3",
    "@types/configstore": "6.0.2",
    "@types/debug": "4.1.12",
    "@types/diff": "5.2.0",
    "@types/escape-html": "1.0.4",
    "@types/inquirer": "9.0.9",
    "@types/libsodium-wrappers": "0.7.14",
    "@types/node": "18.19.130",
    "@types/sax": "1.2.7",
    "@types/semver": "7.7.1",
    "@types/sql.js": "1.4.9",
    "@types/tmp": "0.2.6",
    "@types/vscode": "1.99.0",
    "@types/ws": "8.18.1",
    "@types/xml2js": "0.4.14",
    "@types/yargs": "17.0.34",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "6.5.0",
    "@typescript-eslint/parser": "7.17.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vscode/test-electron": "2.5.2",
    "@vscode/vsce": "3.7.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "0.24.0",
    "nock": "14.0.10",
    "ovsx": "0.10.7",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
